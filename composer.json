{
    "name": "whatwedo/import-bundle",
    "type": "symfony-bundle",
    "description": "Very simple Import",
    "license": "MIT",
    "authors": [
        {
            "name": "Maurizio Monticelli",
            "email": "maurizio@whatwedo.ch"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/validator":                      "^5.0|^6.0",
        "symfony/property-info":                      "^5.0|^6.0",
        "symfony/property-access":                      "^5.0|^6.0",
        "symfony/options-resolver":                      "^5.0|^6.0",
        "symfony/form":                      "^5.0|^6.0",
        "doctrine/orm": "^2.11.0"
    },
    "autoload": {
        "psr-4": {
            "whatwedo\\ImportBundle\\": "src/",
            "whatwedo\\ImportBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "symfony/phpunit-bridge": "^v6.0",
        "symfony/http-kernel": "^v6.0",
        "symfony/config": "^v6.0",
        "symfony/dependency-injection": "^v6.0",
        "symfony/yaml": "^v6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/translation": "^6.0",
        "nyholm/symfony-bundle-test": "^1.8",
        "doctrine/doctrine-bundle": "^2.5.5",
        "whatwedo/php-coding-standard": "dev-develop",
        "zenstruck/foundry": "^1.16",
        "phpoffice/phpspreadsheet": "^1.21",
        "phpstan/phpstan": "^1.5"
    },
    "suggest": {
        "phpoffice/phpspreadsheet": "^1.21"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
